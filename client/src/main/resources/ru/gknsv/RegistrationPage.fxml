<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="backColor" stylesheets="@Background.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.gknsv.controllers.RegistrationPage">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="92.0" prefHeight="400.0" prefWidth="416.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" prefHeight="54.0" prefWidth="222.0" styleClass="sample" stylesheets="@Fonts.css" text="Регистрация" />
            <TextField fx:id="loginField" promptText="Логин" stylesheets="@TextFields.css">
               <styleClass>
                  <String fx:value="text-field" />
                  <String fx:value="smaller" />
               </styleClass></TextField>
            <PasswordField fx:id="passwordField" promptText="Пароль" stylesheets="@TextFields.css">
               <styleClass>
                  <String fx:value="text-field" />
                  <String fx:value="smaller" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></PasswordField>
            <HBox prefHeight="47.0" prefWidth="416.0">
               <children>
                  <AnchorPane fx:id="spawnButton" prefHeight="48.0" prefWidth="38.0" />
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="sexName" layoutY="11.0" styleClass="another" stylesheets="@Fonts.css" text="Label" />
                     </children>
                  </AnchorPane>
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <TextField fx:id="heightField" promptText="Рост" stylesheets="@TextFields.css">
               <styleClass>
                  <String fx:value="text-field" />
                  <String fx:value="smaller" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></TextField>
            <TextField fx:id="weightField" promptText="Вес" stylesheets="@TextFields.css">
               <styleClass>
                  <String fx:value="text-field" />
                  <String fx:value="smaller" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></TextField>
            <TextField fx:id="experienceField" promptText="Стаж" stylesheets="@TextFields.css">
               <styleClass>
                  <String fx:value="text-field" />
                  <String fx:value="smaller" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></TextField>
            <Button mnemonicParsing="false" onAction="#registration" prefHeight="38.0" prefWidth="240.0" stylesheets="@Buttons.css" text="Зарегистрироваться">
               <styleClass>
                  <String fx:value="button-30" />
                  <String fx:value="button-20" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#toAuthorizationPage" prefHeight="38.0" prefWidth="240.0" stylesheets="@Buttons.css" text="Назад">
               <styleClass>
                  <String fx:value="button-30" />
                  <String fx:value="button-20" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
